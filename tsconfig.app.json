{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "dom",
      "dom.iterable",
    ],
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "declaration": true,
    "outDir": "dist/types"
  },
  "include": [
    "src"
  ]
}